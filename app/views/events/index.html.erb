<h1>All Events</h1>
<div class="row">
  <div class="col-md-8">
    <% @events.each do |event| %>
    </br>
      <%= link_to event.name, event_path(event) %>
      </br>
      <small>Start: <%= event.startdate %></small>
      </br>
      <small>End: <%= event.enddate %></small>
    <% end %>
  </div>
  <% if current_user %>
    <div class="col-md-4">
      <%= link_to "New Event", new_event_path, class: 'btn btn-success' %>
    </div>
  <% end %>
</div>